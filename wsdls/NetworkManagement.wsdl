    <wsdl:portType name="NetworkManagement_v1_0">
        <wsdl:operation name="GetNetworkList">
            <wsdl:documentation>
                GetNetworkList
                    Returns list of available network interfaces

                Arguments
                    sessionId (string) : ID of an active session

                Response
                    list of network interfaces, with a flag indicating if they are configurable or not

                Faults
                    "Internal Server Error" : if SOAP request is incorrect or network is not properly configured on the server
            </wsdl:documentation>
            <wsdl:input message="tns:GetNetworkList"/>
            <wsdl:output message="tns:GetNetworkListResponse"/>
        </wsdl:operation>
        <wsdl:operation name="GetNetworkConfig">
            <wsdl:documentation>
                GetNetworkConfig
                    Returns configuration of an interface for example if it is DHCP or statically configured

                Arguments
                    sessionId (string) : ID of an active session
                    interface (string) : Interface name for which we want to obtain configuration

                Response
                    interface (string) : interface name
                    hostname (string)  : hostname used in DHCP requests sent from this interface, used in DHCP configuration
                    type (string)      : DHCP or Static depending on used configuration
                    ipAddress (string) : self explanatory, used for static IP configuration
                    netmask (string)   : self explanatory, used for static IP configuration
                    gateway (string)   : self explanatory, used for static IP configuration

                Faults
                    "Internal Server Error" : if network configuration on the server is incorrect
            </wsdl:documentation>
            <wsdl:input message="tns:GetNetworkConfig"/>
            <wsdl:output message="tns:GetNetworkConfigResponse"/>
        </wsdl:operation>
        <wsdl:operation name="SetNetworkConfig">
            <wsdl:documentation>
                SetNetworkConfig
                    Sets network configuration for a specific interface for example DHCP or static address

                Arguments
                    sessionId (string) : ID of an active session
                    interface (string) : interface name
                    hostname (string)  : hostname used in DHCP requests send from this interface, used in DHCP configuration
                    type (string)      : DHCP or Static depending on used configuration
                    ipAddress (string) : self explanatory, used for static IP configuration
                    netmask (string)   : self explanatory, used for static IP configuration
                    gateway (string)   : self explanatory, used for static IP configuration

                Response
                    Confirmation that configuration has been applied

                Faults
                    "Invalid argument"      : if provided network configuration is incorrect
                    "Missing Argument"      : if SOAP request is not complete
                    "Internal Server Error" : if case of low memory errors and similar
            </wsdl:documentation>
            <wsdl:input message="tns:SetNetworkConfig"/>
            <wsdl:output message="tns:SetNetworkConfigResponse"/>
        </wsdl:operation>
        <wsdl:operation name="GetNetworkInfo">
            <wsdl:documentation>
                GetNetworkInfo
                    GetNetworkInfo - returns current settings for specific interface like IP address or mask

                Arguments
                    sessionId (string)       : ID of active session
                    interface (string)       : interface name

                Response
                    interface (string)                       : interface name
                    active (boolean)                         : true/false flag
                    ipAddress (string)                       : IP address currently assigned to the interface
                    netmask (string)                         : netmask currently assigned to the interface
                    hardwareAddress (string)                 : interface MAC address
                    networkAliasList (ArrayOfNetworkAddress) : network address aliases currently assigned to the interface

                Faults
                    "Missing Argument"      : if SOAP request is not complete
                    "Internal Server Error" : if case of low memory or network misconfiguration
            </wsdl:documentation>
            <wsdl:input message="tns:GetNetworkInfo"/>
            <wsdl:output message="tns:GetNetworkInfoResponse"/>
        </wsdl:operation>
    </wsdl:portType>
