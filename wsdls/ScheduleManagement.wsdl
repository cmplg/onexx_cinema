    <wsdl:portType name="ScheduleManagement_v1_0">
        <wsdl:operation name="CreateSchedule">
            <wsdl:documentation>
                CreateSchedule
                    Create a schedule with provided information

                Arguments
                    sessionId (com:SessionIdentifier) returned by SessionManagement/Login
                    scheduleInfo (sch:ScheduleInfo) information to populate the schedule with
                    checkForOverlapping (optional xsd:boolean) check if new schedule is overlapping with existing

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred
                    "Invalid argument"      : If SPL ID or datetime string in scheduleInfo is incorrect
                    "Missing argument"      : If request does not contain scheduleInfo
                    "Not authenticated"     : If inactive/bad sessionID passed
                    "Permission denied"     : If user not privileged to perform operation
            </wsdl:documentation>
            <wsdl:input message="tns:CreateSchedule"/>
            <wsdl:output message="tns:CreateScheduleResponse"/>
        </wsdl:operation>
        <wsdl:operation name="GetScheduleList">
            <wsdl:documentation>
                GetScheduleList
                    Get a list of available schedules by their ID

                Arguments
                    sessionId (com:SessionIdentifier) returned by SessionManagement/Login
                    dateFrom (optional xsd:dateTime) return only schedules younger than
                    dateTo (optional xsd:dateTime) return only schedules older than

                Response
                    scheduleList (sch:ArrayOfScheduleId) list of found schedule IDs

                Faults
                    "Internal server error" : If a system error occurred
                    "Invalid argument"      : If syntax of datetime string is incorrect
                    "Not authenticated"     : If inactive/bad sessionID passed
                    "Permission denied"     : If user not privileged to perform operation
            </wsdl:documentation>
            <wsdl:input message="tns:GetScheduleList"/>
            <wsdl:output message="tns:GetScheduleListResponse"/>
        </wsdl:operation>
        <wsdl:operation name="DeleteSchedule">
            <wsdl:documentation>
                DeleteSchedule
                    Delete a schedule with provided ID

                Arguments
                    sessionId (com:SessionIdentifier) returned by SessionManagement/Login
                    scheduleId (sch:ScheduleId) ID of schedule to delete

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred
                    "Not found"             : If schedule with provided ID does not exist
                    "Missing argument"      : If request does not contain scheduleId
                    "Not authenticated"     : If inactive/bad sessionID passed
                    "Permission denied"     : If user not privileged to perform operation
            </wsdl:documentation>
            <wsdl:input message="tns:DeleteSchedule"/>
            <wsdl:output message="tns:DeleteScheduleResponse"/>
        </wsdl:operation>
        <wsdl:operation name="GetScheduleInfo">
            <wsdl:documentation>
                GetScheduleInfo
                    Get the info of a schedule

                Arguments
                    sessionId (com:SessionIdentifier) returned by SessionManagement/Login
                    scheduleId (sch:ScheduleId) ID of schedule to get the info of

                Response
                    scheduleInfo (sch:ScheduleInfo) schedule information

                Faults
                    "Internal server error" : If a system error occurred
                    "Not found"             : If schedule with provided ID does not exist
                    "Missing argument"      : If request does not contain scheduleId
                    "Not authenticated"     : If inactive/bad sessionID passed
                    "Permission denied"     : If user not privileged to perform operation
            </wsdl:documentation>
            <wsdl:input message="tns:GetScheduleInfo"/>
            <wsdl:output message="tns:GetScheduleInfoResponse"/>
        </wsdl:operation>
        <wsdl:operation name="GetSchedulerStatus">
            <wsdl:documentation>
                GetSchedulerStatus
                    Get the status of Scheduler

                Arguments
                    sessionId (com:SessionIdentifier) returned by SessionManagement/Login

                Response
                    schedulerStatus (sch:SchedulerStatusInfo) one of: Running, Stopped, Unknown

                Faults
                    "Internal server error" : If a system error occurred
