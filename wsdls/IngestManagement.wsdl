    <wsdl:portType name="IngestManagement_v1_0">
        <wsdl:operation name="CreateIngestTask">
            <wsdl:documentation>
                CreateIngestTask
                    Creates a task to ingest the content specified using the parameters

                Arguments
                    sessionId (com:SessionIdentifier) returned by SessionManagement/Login
                    taskParameters (ngs:IngestTaskParameters) specify parameters for this task (uri, contentType, ...). At least uri and contentType shall be specified
                    taskProperties (ngs:IngestTaskProperties) specify properties for this task (description, creator, ...). All fields are optional.

                Response
                    taskId (ngs:TaskId) ID of the task created

                Faults
                    "not authenticated" : if inactive/bad sessionID passed
                    "permission denied": if user is not privileged to perform the operation
                    "internal server error": if a system error occurred
                    "missing mandatory argument '[X]'": if [X] argument is missing
                    "syntax error": if the syntax of either taskParameters or taskProperties is invalid
            </wsdl:documentation>
            <wsdl:input message="tns:CreateIngestTask"/>
            <wsdl:output message="tns:CreateIngestTaskResponse"/>
        </wsdl:operation>
        <wsdl:operation name="PauseIngestTask">
            <wsdl:documentation>
                PauseIngestTask
                    Pauses the specified ingest task

                Arguments
                    sessionId (com:SessionIdentifier) returned by SessionManagement/Login
                    taskId (ngs:TaskId) returned by CreateIngestTask or GetIngestTaskList

                Response
                    Empty

                Faults
                    "not authenticated" : if inactive/bad sessionID passed
                    "permission denied": if user is not privileged to perform the operation
                    "internal server error": if a system error occurred
                    "missing mandatory argument '[X]'": if [X] argument is missing
                    "invalid argument '[X]'": if [X] argument is in wrong format
            </wsdl:documentation>
            <wsdl:input message="tns:PauseIngestTask"/>
            <wsdl:output message="tns:PauseIngestTaskResponse"/>
        </wsdl:operation>
        <wsdl:operation name="ResumeIngestTask">
            <wsdl:documentation>
                ResumeIngestTask
                    Resumes an ingest task paused using PauseIngestTask

                Arguments
                    sessionId (com:SessionIdentifier) returned by SessionManagement/Login
                    taskId (ngs:TaskId) returned by CreateIngestTask or GetIngestTaskList

                Response
                    Empty

                Faults
                    "not authenticated" : if inactive/bad sessionID passed
                    "permission denied": if user is not privileged to perform the operation
                    "internal server error": if a system error occurred
                    "missing mandatory argument '[X]'": if [X] argument is missing
                    "invalid argument '[X]'": if [X] argument is in wrong format
            </wsdl:documentation>
            <wsdl:input message="tns:ResumeIngestTask"/>
            <wsdl:output message="tns:ResumeIngestTaskResponse"/>
        </wsdl:operation>
        <wsdl:operation name="CancelIngestTask">
            <wsdl:documentation>
                CancelIngestTask
                    Cancels the specified ingest task

                Arguments
                    sessionId (com:SessionIdentifier) returned by SessionManagement/Login
                    taskId (ngs:TaskId) returned by CreateIngestTask or GetIngestTaskList

                Response
                    Empty

                Faults
                    "not authenticated" : if inactive/bad sessionID passed
                    "permission denied": if user is not privileged to perform the operation
                    "internal server error": if a system error occurred
                    "missing mandatory argument '[X]'": if [X] argument is missing
                    "invalid argument '[X]'": if [X] argument is in wrong format
            </wsdl:documentation>
            <wsdl:input message="tns:CancelIngestTask"/>
            <wsdl:output message="tns:CancelIngestTaskResponse"/>
        </wsdl:operation>
        <wsdl:operation name="DeleteIngestTask">
            <wsdl:documentation>
                DeleteIngestTask
                    Removes the specified ingest task from the ingest tasks database

                Arguments
                    sessionId (com:SessionIdentifier) returned by SessionManagement/Login
                    taskId (ngs:TaskId) returned by CreateIngestTask or GetIngestTaskList

                Response
                    Empty

                Faults
                    "not authenticated" : if inactive/bad sessionID passed
                    "permission denied": if user is not privileged to perform the operation
                    "internal server error": if a system error occurred
                    "missing mandatory argument '[X]'": if [X] argument is missing
                    "invalid argument '[X]'": if [X] argument is in wrong format
            </wsdl:documentation>
            <wsdl:input message="tns:DeleteIngestTask"/>
            <wsdl:output message="tns:DeleteIngestTaskResponse"/>
        </wsdl:operation>
        <wsdl:operation name="UpdateIngestTask">
            <wsdl:documentation>
                UpdateIngestTask
                    Updates ingest task properties

                Arguments
                    sessionId (com:SessionIdentifier) returned by SessionManagement/Login
                    taskId (ngs:TaskId) returned by CreateIngestTask or GetIngestTaskList
                    taskProperties (ngs:IngestTaskProperties) specify properties for this task (description, creator, ...). All fields are optional.

                Response
                    Empty

                Faults
                    "not authenticated" : if inactive/bad sessionID passed
                    "permission denied": if user is not privileged to perform the operation
                    "internal server error": if a system error occurred
                    "missing mandatory argument '[X]'": if [X] argument is missing
                    "syntax error": if the syntax of taskProperties is invalid
                    "invalid argument '[X]'": if [X] argument is in wrong format
            </wsdl:documentation>
            <wsdl:input message="tns:UpdateIngestTask"/>
            <wsdl:output message="tns:UpdateIngestTaskResponse"/>
        </wsdl:operation>
