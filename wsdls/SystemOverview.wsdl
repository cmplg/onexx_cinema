        <wsdl:port name="SOAPOverHTTP" binding="tns:SystemOverview_v1_0_SOAPBinding">
            <soap:address location="http://10.201.22.34:10000/dc/dcp/ws/v1/SystemOverview" />
        </wsdl:port>
    </wsdl:service>
</wsdl:definitions>
