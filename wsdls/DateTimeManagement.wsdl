					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="SetTime">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="sessionId" type="com:SessionIdentifier"/>
						<xsd:element name="clockId" type="xsd:token" minOccurs="0"/>
