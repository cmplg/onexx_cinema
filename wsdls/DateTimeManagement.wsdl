	<wsdl:portType name="DateTimeManagement_v1_0">
		<wsdl:operation name="GetTime">
			<wsdl:documentation>
                GetTime
                    Get time

                Arguments
                    sessionId (com:SessionIdentifier)  returned by SessionManagement/Login
                    clockId (xsd:token) IGNORED. Hardware Clock as default

                Response
                    time: (xsd:dateTime) time (YYYY-MM-DD hh:mm:ss+HH:MM eg. 2022-02-24 05:07:00+02:00).

                Faults
                    "missing mandatory argument '[X]'": if [X] argument missed
                    "Internal server error" : If a system error occurred
                    "not authenticated" : If inactive/bad sessionID passed
			</wsdl:documentation>
			<wsdl:input message="tns:GetTime"/>
			<wsdl:output message="tns:GetTimeResponse"/>
		</wsdl:operation>
		<wsdl:operation name="SetTime">
			<wsdl:documentation>
                SetTime
                    Set time

                Arguments
                    sessionId (com:SessionIdentifier)  returned by SessionManagement/Login
                    clockId (xsd:token) IGNORED. Hardware Clock as default
                    time: (xsd:dateTime) time to be set in current timezone (YYYY-MM-DDThh:mm:ss eg. 2022-02-24T03:07:00)
                            or by time string with timezone included in format YYYY-MM-DDThh:mm:ss+ZZ:zz (eg. 2021-06-20T08:06:01-05:00).

                Response
                    return nothing on success.

                Faults
                    "missing mandatory argument '[X]'": if [X] argument missed
                    "invalid argument '[X]'" : if [X] argument in wrong format
                    "permission denied" : If user not privileged to perform operation
                    "Internal server error" : If a system error occurred
                    "not authenticated" : If inactive/bad sessionID passed
                    "undefined error" : if undefined error occured
			</wsdl:documentation>
			<wsdl:input message="tns:SetTime"/>
			<wsdl:output message="tns:SetTimeResponse"/>
		</wsdl:operation>
		<wsdl:operation name="GetTimeZone">
			<wsdl:documentation>
                GetTimeZone
                    Get Time Zone

                Arguments
                    sessionId (com:SessionIdentifier)  returned by SessionManagement/Login

                Response
                    timezone (xsd:string)  like: Europe/Paris
                    timeShift (xsd:int) time shift in minute like: 60
                    scheduleDST (xsd:string) full date time like: Sun Mar 26 01:59:59 2017 CET

                Faults
                    "Internal server error" : If a system error occurred
                    "not authenticated" : If inactive/bad sessionID passed
			</wsdl:documentation>
			<wsdl:input message="tns:GetTimeZone"/>
			<wsdl:output message="tns:GetTimeZoneResponse"/>
		</wsdl:operation>
		<wsdl:operation name="SetTimeZone">
			<wsdl:documentation>
                SetTimeZone
                    Set Time Zone

                Arguments
                    sessionId (com:SessionIdentifier)  returned by SessionManagement/Login
                    timezone (xsd:string)  like: Europe/Paris

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred
                    "invalid argument '[X]'" : if [X] argument has wrong format or value (eg. non existing timezone)
                    "not authenticated" : If inactive/bad sessionID passed
			</wsdl:documentation>
			<wsdl:input message="tns:SetTimeZone"/>
			<wsdl:output message="tns:SetTimeZoneResponse"/>
		</wsdl:operation>
		<wsdl:operation name="GetNTPStatus">
			<wsdl:documentation>
    			GetNTPStatus
                    Get NTP Status data

                Arguments
                    sessionId (com:SessionIdentifier)  returned by SessionManagement/Login

                Response
                    serverList (xsd:string) all NTP servers currently configured as a source for NTP service separated by a space
                    lastSyncServer (xsd:string) server used for the last successfull NTP synchronisation
                    lastSyncDate (xsd:string) date when the last successfull NTP synchronisation occured
                    lastSyncOffset (xsd:string) synchronisation offset detected during last successfull synchronisation, empty when no DST change scheduled

                Faults
                    "Internal server error" : If a system error occurred
                    "not authenticated" : If inactive/bad sessionID passed
			</wsdl:documentation>
			<wsdl:input message="tns:GetNTPStatus"/>
			<wsdl:output message="tns:GetNTPStatusResponse"/>
		</wsdl:operation>
	</wsdl:portType>
