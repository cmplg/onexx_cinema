    <wsdl:portType name="PowerManagement_v1_0">
        <wsdl:operation name="Shutdown">
            <wsdl:documentation>
                 Shutdown
                    Shutdown the system after a delay.

                Arguments
                    sessionId (com:SessionIdentifier)  returned by SessionManagement/Login
                    delayMinutes (integer) number of minutes before shutting down the system (where 0 means now)

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred

            </wsdl:documentation>
            <wsdl:input message="tns:Shutdown"/>
            <wsdl:output message="tns:ShutdownResponse"/>
        </wsdl:operation>
        <wsdl:operation name="Reboot">
            <wsdl:documentation>
                 Reboot
                    Reboot the system after a delay.

                Arguments
                    sessionId (com:SessionIdentifier)  returned by SessionManagement/Login
                    delayMinutes (integer) number of minutes before rebooting the system (where 0 means now)

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred

            </wsdl:documentation>
            <wsdl:input message="tns:Reboot"/>
            <wsdl:output message="tns:RebootResponse"/>
        </wsdl:operation>
        <wsdl:operation name="CancelShutdown">
            <wsdl:documentation>
                 CancelShutdown
                    Cancel the shutdown of the system.

                Arguments
                    sessionId (com:SessionIdentifier)  returned by SessionManagement/Login

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred

            </wsdl:documentation>
            <wsdl:input message="tns:CancelShutdown"/>
            <wsdl:output message="tns:CancelShutdownResponse"/>
        </wsdl:operation>
        <wsdl:operation name="CancelReboot">
            <wsdl:documentation>
                 CancelReboot
                    Cancel the reboot of the system.

                Arguments
                    sessionId (com:SessionIdentifier)  returned by SessionManagement/Login

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred

            </wsdl:documentation>
            <wsdl:input message="tns:CancelReboot"/>
            <wsdl:output message="tns:CancelRebootResponse"/>
        </wsdl:operation>
        <wsdl:operation name="Standby">
            <wsdl:documentation>
                 Standby
                    Standy the system.

                Arguments
                    sessionId (com:SessionIdentifier)  returned by SessionManagement/Login

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred

            </wsdl:documentation>
            <wsdl:input message="tns:Standby"/>
            <wsdl:output message="tns:StandbyResponse"/>
        </wsdl:operation>
        <wsdl:operation name="Wakeup">
            <wsdl:documentation>
                 Wakeup
                    Wakeup the system after a standby.

                Arguments
                    sessionId (com:SessionIdentifier)  returned by SessionManagement/Login

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred

            </wsdl:documentation>
            <wsdl:input message="tns:Wakeup"/>
            <wsdl:output message="tns:WakeupResponse"/>
        </wsdl:operation>
