    <wsdl:portType name="SystemInformation_v1_0">
        <wsdl:operation name="GetProductInformation">
            <wsdl:documentation>
                GetProductInformation
                    Get Product Informaiton

                Arguments
                    sessionId   (com:SessionIdentifier)  returned by SessionManagement/Login.

                Response
                    productInformation (sys:ProductInformationInfo) Product Information.

                Faults
                    "Internal server error" : If a system error occurred
            </wsdl:documentation>
            <wsdl:input message="tns:GetProductInformation"/>
            <wsdl:output message="tns:GetProductInformationResponse"/>
        </wsdl:operation>
        <wsdl:operation name="GetSoftwareInventoryList">
            <wsdl:documentation>
                GetSoftwareInventoryList
                    Get Software inventory list

                Arguments
                    sessionId   (com:SessionIdentifier)  returned by SessionManagement/Login.

                Response
                    softwarePartList (sys:ArrayOfSoftwarePart) List of software part. 

                Faults
                    "Internal server error" : If a system error occurred
            </wsdl:documentation>
            <wsdl:input message="tns:GetSoftwareInventoryList"/>
            <wsdl:output message="tns:GetSoftwareInventoryListResponse"/>
        </wsdl:operation>
        <wsdl:operation name="GetHardwareInventoryList">
            <wsdl:documentation>
                GetHardwareInventoryList
                    Gets Hardware inventory list

                Arguments
                    sessionId   (com:SessionIdentifier)  returned by SessionManagement/Login.

                Response
                    hardwarePartList (sys:ArrayOfHardwarePart) List of hardware part.

                Faults
                    "Internal server error" : If a system error occurred
            </wsdl:documentation>
            <wsdl:input message="tns:GetHardwareInventoryList"/>
            <wsdl:output message="tns:GetHardwareInventoryListResponse"/>
        </wsdl:operation>
        <wsdl:operation name="GetHostname">
            <wsdl:documentation>
                GetHostname
                    Gets SMS board network hostname and screen name for one of IMBs connected to the SMS

                Arguments
                    sessionId   (com:SessionIdentifier)  returned by SessionManagement/Login.

                Response
                    hostname    Network hostname for SMS board
                    screenName  Screen Name for one of IMBs. IMB is selected by Channel Management functionality.

                Faults
                    "Internal server error" : If a system error occurred
            </wsdl:documentation>
            <wsdl:input message="tns:GetHostname"/>
            <wsdl:output message="tns:GetHostnameResponse"/>
        </wsdl:operation>
        <wsdl:operation name="SetHostname">
            <wsdl:documentation>
                SetHostname
                    Sets SMS board network hostname and/or screen name for one of IMSs connected to the SMS

                Arguments
                    sessionId   (com:SessionIdentifier)  returned by SessionManagement/Login
                    hostname    Network hostname for SMS board
                    screenName  Screen Name for one of IMBs. IMB is selected by Channel Management functionality.

                Response
                    Return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred
            </wsdl:documentation>
            <wsdl:input message="tns:SetHostname"/>
            <wsdl:output message="tns:SetHostnameResponse"/>
        </wsdl:operation>
        <wsdl:operation name="GetCertificateList">
            <wsdl:documentation>
                GetCertificateList
                    Gets certificate list

                Arguments
                    sessionId   (com:SessionIdentifier)  returned by SessionManagement/Login.

                Response
                    certificateList (sys:ArrayOfCertificate) List of certificat.

                Faults
                    "Internal server error" : If a system error occurred
            </wsdl:documentation>
            <wsdl:input message="tns:GetCertificateList"/>
            <wsdl:output message="tns:GetCertificateListResponse"/>
        </wsdl:operation>
        <wsdl:operation name="GetFeatureList">
            <wsdl:documentation>
                GetFeatureList
                    Gets feature list

                Arguments
                    sessionId   (com:SessionIdentifier)  returned by SessionManagement/Login.

                Response
                    featureList (sys:ArrayOfFeatureTag) List of feature.

                Faults
                    "Internal server error" : If a system error occurred
            </wsdl:documentation>
            <wsdl:input message="tns:GetFeatureList"/>
            <wsdl:output message="tns:GetFeatureListResponse"/>
        </wsdl:operation>
    </wsdl:portType>
