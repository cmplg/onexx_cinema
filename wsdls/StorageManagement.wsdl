   <wsdl:portType name="StorageManagement_v1_0">
      <wsdl:operation name="GetStorageList">
         <wsdl:documentation>
            GetStorageList
               Return storage list

            Arguments
               sessionId (string) returned by SessionManagement/Login

            Response
               storageList (ArrayOfStorage) list of storage (/data, /opt, /dolby)
               with capacity and percentage of filled space

            Faults
               "Session not found" : If the session id is invalid.
         </wsdl:documentation>
         <wsdl:input message="tns:GetStorageList" />
         <wsdl:output message="tns:GetStorageListResponse" />
      </wsdl:operation>
      <wsdl:operation name="GetStorageUnitInfo">
         <wsdl:documentation>
            GetStorageUnitInfo
               Return storage unit info

            Arguments
               sessionId (string) returned by SessionManagement/Login
               name (string) name of storage (eg. md, hdd, rootdev)

            Response
               storageUnitInfo (StorageUnitInfo) status of storage unit
               (eg. capacity, state, raid status)

            Faults
               "Session not found" : If the session id is invalid.
               "Not found" : If storage unit does not exist
         </wsdl:documentation>
         <wsdl:input message="tns:GetStorageUnitInfo" />
         <wsdl:output message="tns:GetStorageUnitInfoResponse" />
      </wsdl:operation>
      <wsdl:operation name="AddActiveUnit">
         <wsdl:documentation>
            AddActiveUnit
               Add a device to RAID array

            Arguments
               sessionId (string) returned by SessionManagement/Login
               storageUnit (string) storage unit under /dev (eg. md0, md1)
               activeUnit (string) device in /dev to add to RAID (eg. raid_hdd1)

            Response
               return nothing on success.

            Faults
               "Session not found" : If the session id is invalid.
               "Invalid argument" : If storageUnit or activeUnit can't be found
         </wsdl:documentation>
         <wsdl:input message="tns:AddActiveUnit" />
         <wsdl:output message="tns:AddActiveUnitResponse" />
      </wsdl:operation>
      <wsdl:operation name="RemoveActiveUnit">
         <wsdl:documentation>
            RemoveActiveUnit
               Remove a device from RAID array

            Arguments
               sessionId (string) returned by SessionManagement/Login
               storageUnit (string) storage unit under /dev (eg. md0, md1)
               activeUnit (string) device in /dev to remove from RAID (eg. raid_hdd1)

            Response
               return nothing on success.

            Faults
               "Session not found" : If the session id is invalid.
               "Invalid argument" : If storageUnit or activeUnit can't be found
         </wsdl:documentation>
         <wsdl:input message="tns:RemoveActiveUnit" />
         <wsdl:output message="tns:RemoveActiveUnitResponse" />
      </wsdl:operation>
      <wsdl:operation name="StartStorageInit">
         <wsdl:documentation>
            StartStorageInit
               Start/reinitialise RAID storage

            Arguments
               sessionId (string) returned by SessionManagement/Login
               storageName (optional string) storage share name ("/data" or "/opt") to be
               reinitialised. When not specified both will be (re)initialised.
               driveCount (optional int) DEPRECATED ! Will cause service fail.

            Response
               return nothing on success.

            Faults
               "Session not found" : If the session id is invalid.
               "Resource busy" : If storage init is already running
         </wsdl:documentation>
         <wsdl:input message="tns:StartStorageInit" />
         <wsdl:output message="tns:StartStorageInitResponse" />
      </wsdl:operation>
      <wsdl:operation name="GetStorageInitStatus">
         <wsdl:documentation>
            GetStorageInitStatus
               Get status of storage initialization

            Arguments
               sessionId (string) returned by SessionManagement/Login

            Response
               status (StorageInitStatus) status of init operation (eg. Idle, Running)
               logs (ArrayOfString) logs reported from init operation

            Faults
               "Session not found" : If the session id is invalid.
         </wsdl:documentation>
         <wsdl:input message="tns:GetStorageInitStatus" />
         <wsdl:output message="tns:GetStorageInitStatusResponse" />
      </wsdl:operation>
      <wsdl:operation name="GetMainStorageTaskStatuses">
         <wsdl:documentation>
            GetMainStorageTaskStatuses

            Arguments
               sessionId (com:SessionIdentifier)
               action (str:ActionType)

            Response
            statuses (str:ArrayOfProcessStatus)

            Faults
               "Internal server error" : If the server detects an error
               "Resource busy" : If CP is unavailable.
         </wsdl:documentation>
         <wsdl:input message="tns:GetMainStorageTaskStatuses" />
         <wsdl:output message="tns:GetMainStorageTaskStatusesResponse" />
      </wsdl:operation>
      <wsdl:operation name="StartMainStorageTask">
         <wsdl:documentation>
            StartMainStorageTask
               Start specified "action" on the main storage NAS
               Action "repair" and "reset" are done on current applied configuration (other fields may
               have random value)
               Action "test_access", "test_config" and "initial_setup" are done using parameters of the
               current command

            Arguments
               sessionId (com:SessionIdentifier)
               action (str:ActionType)
               ipAddress (com:IpAddress)
               protocol (str:Protocol)
               port (xsd:int)
               password (xsd:string)

            Response

            Faults
               "Internal server error" : If the server detects an error
         </wsdl:documentation>
         <wsdl:input message="tns:StartMainStorageTask" />
         <wsdl:output message="tns:StartMainStorageTaskResponse" />
      </wsdl:operation>
