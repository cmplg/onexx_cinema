    <wsdl:portType name="ShowControl_v1_0">
        <wsdl:operation name="Play">
            <wsdl:documentation>
                Play
                    Start the current CPL loaded in the current ShowPlaylist

                Arguments
                    sessionId (string) returned by SessionManagement/Login

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred

            </wsdl:documentation>
            <wsdl:input message="tns:Play"/>
            <wsdl:output message="tns:PlayResponse"/>
        </wsdl:operation>
        <wsdl:operation name="Pause">
            <wsdl:documentation>
                Pause
                    Pause the current CPL loaded in the current ShowPlaylist

                Arguments
                    sessionId (string) returned by SessionManagement/Login

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred

            </wsdl:documentation>
            <wsdl:input message="tns:Pause"/>
            <wsdl:output message="tns:PauseResponse"/>
        </wsdl:operation>
        <wsdl:operation name="Eject">
            <wsdl:documentation>
                Eject
                    Eject the current ShowPlaylist

                Arguments
                    sessionId (string) returned by SessionManagement/Login

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred
            </wsdl:documentation>
            <wsdl:input message="tns:Eject"/>
            <wsdl:output message="tns:EjectResponse"/>
        </wsdl:operation>
        <wsdl:operation name="SkipForward">
            <wsdl:documentation>
                SkipForward
                    Skip Forward to the new CPL in the current ShowPlaylist
                    Note: Show has to be in Pause

                Arguments
                    sessionId (string) returned by SessionManagement/Login

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred
                    "Not authenticated"     : If inactive/bad sessionID passed
                    "Permission denied"     : If user not privileged to perform operation
            </wsdl:documentation>
            <wsdl:input message="tns:SkipForward"/>
            <wsdl:output message="tns:SkipForwardResponse"/>
        </wsdl:operation>
        <wsdl:operation name="SkipBackward">
            <wsdl:documentation>
                SkipBackward
                    Skip backward to the previous CPL in the current ShowPlaylist
                    Note: Show has to be in Pause

                Arguments
                    sessionId (string) returned by SessionManagement/Login

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred
                    "Not authenticated"     : If inactive/bad sessionID passed
                    "Permission denied"     : If user not privileged to perform operation
            </wsdl:documentation>
            <wsdl:input message="tns:SkipBackward"/>
            <wsdl:output message="tns:SkipBackwardResponse"/>
        </wsdl:operation>
        <wsdl:operation name="JumpForward">
            <wsdl:documentation>
                JumpForward
                    Jump forward in the current loaded CPL
                    Note: Show has to be in Pause

                Arguments
                    sessionId (string) returned by SessionManagement/Login
                    offset (integer)  Jump offset in second

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred
                    "Invalid argument"      : If offset argument is not of integer type
                    "Missing argument"      : If argument is not present in request
                    "Not authenticated"     : If inactive/bad sessionID passed
                    "Permission denied"     : If user not privileged to perform operation
            </wsdl:documentation>
            <wsdl:input message="tns:JumpForward"/>
            <wsdl:output message="tns:JumpForwardResponse"/>
        </wsdl:operation>
        <wsdl:operation name="JumpBackward">
            <wsdl:documentation>
                JumpBackward
                    Jump backward in the current loaded CPL
                    Note: Show has to be in Pause

                Arguments
                    sessionId (string) returned by SessionManagement/Login
                    offset (integer)  Jump offset in second

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred
                    "Invalid argument"      : If offset argument is not of integer type
                    "Missing argument"      : If argument is not present in request
                    "Not authenticated"     : If inactive/bad sessionID passed
                    "Permission denied"     : If user not privileged to perform operation
            </wsdl:documentation>
            <wsdl:input message="tns:JumpBackward"/>
            <wsdl:output message="tns:JumpBackwardResponse"/>
        </wsdl:operation>
        <wsdl:operation name="JumpTimecode">
            <wsdl:documentation>
                JumpTimecode
                    Jump to Timecode in the current loaded CPL
                    Note: Show has to be in Pause

                Arguments
                    sessionId (string) returned by SessionManagement/Login
                    timecode (integer) Timecode to jump at (hh:mm:ss:ff)

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred
                    "Invalid argument"      : If timecode argument is not in correct format
                    "Missing argument"      : If argument is not present in request
                    "Not authenticated"     : If inactive/bad sessionID passed
                    "Permission denied"     : If user not privileged to perform operation
            </wsdl:documentation>
            <wsdl:input message="tns:JumpTimecode"/>
            <wsdl:output message="tns:JumpTimecodeResponse"/>
        </wsdl:operation>
        <wsdl:operation name="JumpElement">
            <wsdl:documentation>
                JumpElement
                    Jump to an Element in the current loaded ShowPlaylist
                    Note: Show has to be in Pause

                Arguments
                    sessionId (string) returned by SessionManagement/Login
                    timecode (integer) Timecode to jump at (hh:mm:ss:ff)

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred
                    "Invalid argument"      : If elementId argument is not of a correct UUID
                    "Missing argument"      : If argument is not present in request
                    "Not authenticated"     : If inactive/bad sessionID passed
                    "Permission denied"     : If user not privileged to perform operation
            </wsdl:documentation>
            <wsdl:input message="tns:JumpElement"/>
            <wsdl:output message="tns:JumpElementResponse"/>
        </wsdl:operation>
        <wsdl:operation name="SetLoopMode">
            <wsdl:documentation>
                SetLoopMode
                    Set the ShowPlaylist loop mode.

                Arguments
                    sessionId (string) returned by SessionManagement/Login
                    loopMode" (sct:LoopModeType) Loop Mode to apply

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred
                    "Invalid argument"      : If elementId argument is not of a correct UUID
                    "Missing argument"      : If argument is not present in request
                    "Not authenticated"     : If inactive/bad sessionID passed
                    "Permission denied"     : If user not privileged to perform operation
            </wsdl:documentation>
            <wsdl:input message="tns:SetLoopMode"/>
            <wsdl:output message="tns:SetLoopModeResponse"/>
        </wsdl:operation>
        <wsdl:operation name="GetLoopMode">
            <wsdl:documentation>
                GetLoopMode
                    Get the ShowPlaylist loop mode.

                Arguments
                    sessionId (string) returned by SessionManagement/Login

                Response
                    loopMode" (sct:LoopModeType) Loop Mode applied

                Faults
                    "Internal server error" : If a system error occurred
                    "Not authenticated"     : If inactive/bad sessionID passed
                    "Permission denied"     : If user not privileged to perform operation
            </wsdl:documentation>
            <wsdl:input message="tns:GetLoopMode"/>
            <wsdl:output message="tns:GetLoopModeResponse"/>
        </wsdl:operation>
        <wsdl:operation name="LoadShowAsset">
            <wsdl:documentation>
                LoadShowAsset
                    Load a ShowPlaylist from UUID

                Arguments
                    sessionId (string) returned by SessionManagement/Login
                    splId (com:Uuid) UUID of ShowPlaylist

                Response
                    loopMode" (sct:LoopModeType) Loop Mode applied

                Faults
                    "Internal server error" : If a system error occurred
            </wsdl:documentation>
            <wsdl:input message="tns:LoadShowAsset"/>
            <wsdl:output message="tns:LoadShowAssetResponse"/>
        </wsdl:operation>
        <wsdl:operation name="LoadClip">
            <wsdl:documentation>
                LoadClip
                    Load a Content Playlist from UUID
                    Note: this will create a new ShowPlaylist with this Content Playlist

                Arguments
                    sessionId (string) returned by SessionManagement/Login
                    clipId (com:Uuid) UUID of Content Playlist

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred
            </wsdl:documentation>
            <wsdl:input message="tns:LoadClip"/>
            <wsdl:output message="tns:LoadClipResponse"/>
        </wsdl:operation>
        <wsdl:operation name="LoadShowContent">
            <wsdl:documentation>
                LoadShowContent
                    Load a Content Playlist from XML Content

                Arguments
                    sessionId (string) returned by SessionManagement/Login
                    content (base64Binary) ShowPlaylist XML content in Base64

                Response
                    return nothing on success.

                Faults
                    "Internal server error" : If a system error occurred
            </wsdl:documentation>
            <wsdl:input message="tns:LoadShowContent"/>
            <wsdl:output message="tns:LoadShowContentResponse"/>
        </wsdl:operation>
        <wsdl:operation name="GetShowStatus">
            <wsdl:documentation>
                GetShowStatus
                    Return ShowPlaylist Status

                Arguments
                    sessionId (string) returned by SessionManagement/Login

                Response
                    showStatus (sct:ShowStatus)  ShowPlaylist Status

                Faults
                    "Internal server error" : If a system error occurred
            </wsdl:documentation>
            <wsdl:input message="tns:GetShowStatus"/>
            <wsdl:output message="tns:GetShowStatusResponse"/>
        </wsdl:operation>
        <wsdl:operation name="GetShowLockStatus">
            <wsdl:documentation>
                GetShowLockStatus
                    Return Show Lock Status

                Arguments
                    sessionId (string) returned by SessionManagement/Login

                Response
                    locked (boolean) true if the current Show is locked
                    [owningLock] (boolean)
                    [peerName] (string)
                    [peerIPAddress] (string)

                Faults
                    "Internal server error" : If a system error occurred
            </wsdl:documentation>
            <wsdl:input message="tns:GetShowLockStatus"/>
            <wsdl:output message="tns:GetShowLockStatusResponse"/>
        </wsdl:operation>
        <wsdl:operation name="GetElementListStatus">
            <wsdl:documentation>
                GetElementListStatus
                    Return show element list status

                Arguments
                    sessionId (string) returned by SessionManagement/Login

                Response
                    elementList (sct:ArrayOfPlaybackElementStatus) - List of Show elements status

                Faults
                    "Internal server error" : If a system error occurred
                    "Missing argument"      : If argument is not present in request
                    "Not authenticated"     : If inactive/bad sessionID passed
                    "Permission denied"     : If user not privileged to perform operation
            </wsdl:documentation>
            <wsdl:input message="tns:GetElementListStatus"/>
            <wsdl:output message="tns:GetElementListStatusResponse"/>
        </wsdl:operation>
        <wsdl:operation name="GetLoadingStatus">
            <wsdl:documentation>
                GetLoadingStatus
                    Return Load SPL status (Percentage, stage description, error code)

                Arguments
                    sessionId (string) returned by SessionManagement/Login

                Response
                    progress  (com:Percentage) - Load SPL progress percentage
                    stage     (string)         - Load SPL stage decription
                    errorCode (unsignedInt)    - Last PlayerErrorCode

                Faults
                    "Internal server error" : If a system error occurred
                    "Missing argument"      : If argument is not present in request
                    "Not authenticated"     : If inactive/bad sessionID passed
                    "Permission denied"     : If user not privileged to perform operation
            </wsdl:documentation>
            <wsdl:input message="tns:GetLoadingStatus"/>
            <wsdl:output message="tns:GetLoadingStatusResponse"/>
        </wsdl:operation>
        <wsdl:operation name="GetErrorStatus">

                        <xsd:element name="date" type="xsd:dateTime"/>
                        <xsd:element name="errorCode" type="xsd:unsignedInt"/>
                        <xsd:element name="cplId" type="com:Uuid"/>
                        <xsd:element name="kdmId" type="com:Uuid"/>
            <wsdl:documentation>
                GetErrorStatus
                    Return Show Error Status

                Arguments
                    sessionId (string) returned by SessionManagement/Login

                Response
                    date (dateTime) when Show error appears
                    errorCode (unsignedInt) Error Code

                        PlayerErrorCodeSuccess                    = 0
                        /* Unknown error         */
                        PlayerErrorCodeUnknown                    = 1
                        PlayerErrorCodeKdmNotFound                = 2
                        PlayerErrorCodeKdmEndOfEngagement         = 3
                        PlayerErrorCodeSmsCommandFailed           = 4

                        PlayerErrorCodeOdeticsdNotRunning         = 5

                        PlayerErrorCodeMissingFirmware            = 6
                        PlayerErrorCodeSelectFirmware             = 7

                        PlayerErrorCodeKdmNoPPV                   = 8

                        PlayerErrorCodeProjectorError             = 0x0A

                        PlayerErrorCodeImbErrorStart              = 0x80
                        /* IMB state             */
                        PlayerErrorCodeImbPhysicalMarriageBroken  = 0x81
                        PlayerErrorCodeImbLogicalMarriageBroken   = 0x82
                        PlayerErrorCodeImbInactiveMarriage        = 0x83
                        PlayerErrorCodeImbServiceDoorOpen         = 0x84
                        PlayerErrorCodeImbServiceDoorNotArmed     = 0x85
                        PlayerErrorCodeImbNotConnected            = 0x86

                        /* cf. AssetLibErrorEnum */
                        PlayerErrorCodeAssetLibErrorStart         = 0x100
                        PlayerErrorCodeAssetLibErrorEnd           = 0x200

                    cplId (com:Uuid) CPL Uuid concerned by the error
                    kdmId (com:Uuid) KDM Uuid concerned by the error

                Faults
                    "Internal server error" : If a system error occurred
            </wsdl:documentation>
            <wsdl:input message="tns:GetErrorStatus"/>
            <wsdl:output message="tns:GetErrorStatusResponse"/>
        </wsdl:operation>
    </wsdl:portType>
