<wsdl:definitions xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="http://www.doremilabs.com/dc/dcp/ws/v1"
	xmlns:v1_0="http://www.doremilabs.com/dc/dcp/ws/v1_0"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:com="http://www.doremilabs.com/dc/dcp/ws/v1/schemas/common"
	xmlns:virt="http://www.doremilabs.com/dc/dcp/ws/v1/schemas/virtualassetmanagement"
	name="VirtualAssetManagement_v1" targetNamespace="http://www.doremilabs.com/dc/dcp/ws/v1">
	<wsdl:documentation>
		Virtual Asset Management provides function to add a virtual asset (ie:
		Live). In order to delete or list virtual asset, use AssetManagement service.
	</wsdl:documentation>
	<wsdl:types>
		<xsd:schema targetNamespace="http://www.doremilabs.com/dc/dcp/ws/v1_0">
			<xsd:import namespace="http://schemas.xmlsoap.org/soap/encoding/" />
			<xsd:import namespace="http://schemas.xmlsoap.org/wsdl/" />
			<xsd:import schemaLocation="Common.xsd"
				namespace="http://www.doremilabs.com/dc/dcp/ws/v1/schemas/common" />
			<xsd:import schemaLocation="VirtualAssetManagement.xsd"
				namespace="http://www.doremilabs.com/dc/dcp/ws/v1/schemas/virtualassetmanagement" />
			<xsd:element name="CreateVirtualClip">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="sessionId" type="com:SessionIdentifier" />
						<xsd:element name="parameters" type="virt:VirtualAssetParameters" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="CreateVirtualClipResponse">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="clipId" type="virt:VirtualClipCreationId" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:schema>
	</wsdl:types>
	<wsdl:message name="CreateVirtualClip">
		<wsdl:part name="parameters" element="v1_0:CreateVirtualClip" />
	</wsdl:message>
	<wsdl:message name="CreateVirtualClipResponse">
		<wsdl:part name="parameters" element="v1_0:CreateVirtualClipResponse" />
	</wsdl:message>
	<wsdl:portType name="VirtualAssetManagement_v1_0">
		<wsdl:operation name="CreateVirtualClip">
			<wsdl:documentation>
				CreateVirtualClip
					Create a Virtual Clip

				Arguments
					sessionId (string) returned by SessionManagement/Login
					parameters (virt:VirtualAssetParameters) define the type, the duration and the name of this virual clip

				Response
					clipId  CPL uuid

				Faults
					"Internal server error"  : If a system error occurred
					"Invalid argument"       : If argument is incorrect
					"Missing argument"       : If argument is not present in request
					"Not authenticated"      : If inactive/bad sessionID passed
					"Permission denied"      : If user not privileged to perform operation
					"Resource not available" : If server is busy
			</wsdl:documentation>
			<wsdl:input message="tns:CreateVirtualClip" />
			<wsdl:output message="tns:CreateVirtualClipResponse" />
		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding name="VirtualAssetManagement_v1_0_SOAPBinding"
		type="tns:VirtualAssetManagement_v1_0">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="CreateVirtualClip">
			<soap:operation soapAction="" style="document" />
			<wsdl:input>
				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="VirtualAssetManagement_v1_0">
		<wsdl:port name="SOAPOverHTTP" binding="tns:VirtualAssetManagement_v1_0_SOAPBinding">
			<soap:address location="http://127.0.0.1:10000/dc/dcp/ws/v1/VirtualAssetManagement" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
